{
  "name": "data-lineage-viz",
  "displayName": "Data Lineage Viz",
  "preview": true,
  "description": "Visualize object dependencies and data lineage in SQL Server Database Projects (.dacpac)",
  "version": "0.7.3",
  "publisher": "datahelper-chwagner",
  "license": "MIT",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Data Science",
    "Visualization",
    "Other"
  ],
  "keywords": [
    "dacpac",
    "sql",
    "lineage",
    "database",
    "dag",
    "visualization",
    "dependency",
    "schema",
    "stored-procedure",
    "sql-server",
    "database-project",
    "data-lineage"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "badges": [
    {
      "url": "https://img.shields.io/badge/license-MIT-blue.svg",
      "href": "https://github.com/ChrisDevRepo/vscode_data_lineage/blob/main/LICENSE",
      "description": "License: MIT"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ChrisDevRepo/vscode_data_lineage"
  },
  "homepage": "https://chrisdevrepo.github.io/vscode_data_lineage/",
  "main": "./out/extension.js",
  "contributes": {
    "languages": [{ "id": "dacpac-sql", "aliases": ["DACPAC SQL"] }],
    "grammars": [{ "language": "dacpac-sql", "scopeName": "source.dacpac-sql", "path": "./syntaxes/dacpac-sql.json" }],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dataLineageViz",
          "title": "Data Lineage",
          "icon": "images/icon.svg"
        }
      ]
    },
    "views": {
      "dataLineageViz": [
        {
          "id": "dataLineageViz.quickActions",
          "name": "Quick Actions",
          "icon": "$(graph)",
          "contextualTitle": "Data Lineage"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "dataLineageViz.quickActions",
        "contents": "Welcome to Data Lineage Viz!\n[Open](command:dataLineageViz.open)"
      }
    ],
    "commands": [
      {
        "command": "dataLineageViz.open",
        "title": "Data Lineage: Open",
        "icon": "$(graph)"
      },
      {
        "command": "dataLineageViz.createParseRules",
        "title": "Data Lineage: Create Parse Rules",
        "icon": "$(file-code)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "dataLineageViz.open",
          "when": "view == dataLineageViz.quickActions",
          "group": "navigation@1"
        }
      ]
    },
    "configuration": {
      "title": "Data Lineage Viz",
      "properties": {
        "dataLineageViz.parseRulesFile": {
          "type": "string",
          "default": "",
          "description": "Path to a custom parseRules.yaml file for SQL body parsing. Relative paths resolve from the workspace root. Leave empty to use built-in defaults.",
          "scope": "resource"
        },
        "dataLineageViz.excludePatterns": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "Regex patterns to exclude objects at import time (matched against full name, e.g. 'dbo\\.tmp_.*', 'staging\\..*'). Excluded objects and their edges are removed before the graph model is built. Changes require re-importing the dacpac.",
          "scope": "resource"
        },
        "dataLineageViz.maxNodes": {
          "type": "number",
          "default": 250,
          "minimum": 10,
          "maximum": 1000,
          "description": "Maximum number of objects allowed for import. The Visualize button is disabled when the selection exceeds this limit. Higher values may impact performance.",
          "scope": "resource"
        },
        "dataLineageViz.layout.direction": {
          "type": "string",
          "enum": [
            "TB",
            "LR"
          ],
          "default": "LR",
          "enumDescriptions": [
            "Top to Bottom (hierarchical)",
            "Left to Right (horizontal flow, default)"
          ],
          "description": "Graph layout direction.",
          "scope": "resource"
        },
        "dataLineageViz.layout.rankSeparation": {
          "type": "number",
          "default": 120,
          "minimum": 20,
          "maximum": 300,
          "description": "Vertical (TB) or horizontal (LR) spacing between ranks/layers.",
          "scope": "resource"
        },
        "dataLineageViz.layout.nodeSeparation": {
          "type": "number",
          "default": 30,
          "minimum": 10,
          "maximum": 200,
          "description": "Spacing between nodes within the same rank.",
          "scope": "resource"
        },
        "dataLineageViz.layout.edgeAnimation": {
          "type": "boolean",
          "default": true,
          "description": "Enable edge animation during trace mode (animated flow lines on traced paths).",
          "scope": "resource"
        },
        "dataLineageViz.layout.highlightAnimation": {
          "type": "boolean",
          "default": false,
          "description": "Enable edge animation when clicking a node outside of trace mode.",
          "scope": "resource"
        },
        "dataLineageViz.trace.defaultUpstreamLevels": {
          "type": "number",
          "default": 3,
          "minimum": 0,
          "maximum": 99,
          "description": "Default number of upstream levels when starting a trace.",
          "scope": "resource"
        },
        "dataLineageViz.trace.defaultDownstreamLevels": {
          "type": "number",
          "default": 3,
          "minimum": 0,
          "maximum": 99,
          "description": "Default number of downstream levels when starting a trace.",
          "scope": "resource"
        },
        "dataLineageViz.logLevel": {
          "type": "string",
          "enum": [
            "info",
            "debug"
          ],
          "default": "info",
          "enumDescriptions": [
            "Log key events only",
            "Verbose logging for troubleshooting"
          ],
          "description": "Log verbosity in the 'Data Lineage Viz' output channel.",
          "scope": "resource"
        },
        "dataLineageViz.edgeStyle": {
          "type": "string",
          "enum": [
            "default",
            "smoothstep",
            "step",
            "straight"
          ],
          "default": "default",
          "enumDescriptions": [
            "Smooth bezier curves (default)",
            "Smooth step edges with rounded corners",
            "Step edges with sharp corners",
            "Straight lines"
          ],
          "description": "Style of edges/connections between nodes.",
          "scope": "resource"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build:ext && npm run build:webview",
    "build": "npm run build:ext && npm run build:webview",
    "build:ext": "node esbuild.config.mjs",
    "build:webview": "vite build",
    "dev": "vite",
    "watch": "npm run watch:ext",
    "watch:ext": "node esbuild.config.mjs --watch",
    "lint": "eslint src --ext ts,tsx",
    "test": "tsx test/engine.test.ts",
    "test:integration": "npm run build && tsx --tsconfig test/tsconfig.json test/runTest.ts"
  },
  "dependencies": {
    "@dagrejs/dagre": "^1.1.4",
    "@xyflow/react": "^12.4.0",
    "fast-xml-parser": "^5.3.4",
    "graphology": "^0.25.4",
    "graphology-traversal": "^0.3.1",
    "graphology-types": "^0.24.7",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/dagre": "^0.7.52",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.11.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/vscode": "^1.85.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vscode/test-electron": "^2.5.2",
    "autoprefixer": "^10.4.20",
    "esbuild": "^0.25.0",
    "glob": "^13.0.0",
    "mocha": "^11.7.5",
    "postcss": "^8.4.49",
    "sharp": "^0.34.5",
    "svgo": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0"
  }
}
