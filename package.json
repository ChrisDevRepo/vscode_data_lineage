{
  "name": "data-lineage-viz",
  "displayName": "Data Lineage Viz",
  "preview": true,
  "description": "Visualize object dependencies and data lineage from .dacpac files or by importing from SQL Server, Azure SQL, Fabric DW, and Synapse databases",
  "version": "0.9.0",
  "publisher": "datahelper-chwagner",
  "license": "MIT",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Data Science",
    "Visualization",
    "Other"
  ],
  "keywords": [
    "dacpac",
    "sql",
    "lineage",
    "database",
    "dag",
    "visualization",
    "dependency",
    "schema",
    "stored-procedure",
    "sql-server",
    "azure-sql",
    "fabric",
    "synapse",
    "database-project",
    "data-lineage"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "badges": [
    {
      "url": "https://img.shields.io/badge/license-MIT-blue.svg",
      "href": "https://github.com/ChrisDevRepo/vscode_data_lineage/blob/main/LICENSE",
      "description": "License: MIT"
    }
  ],
  "bugs": {
    "url": "https://github.com/ChrisDevRepo/vscode_data_lineage/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChrisDevRepo/vscode_data_lineage"
  },
  "homepage": "https://chrisdevrepo.github.io/vscode_data_lineage/",
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "dacpac-sql",
        "aliases": [
          "DACPAC SQL"
        ]
      }
    ],
    "grammars": [
      {
        "language": "dacpac-sql",
        "scopeName": "source.dacpac-sql",
        "path": "./syntaxes/dacpac-sql.json"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dataLineageViz",
          "title": "Data Lineage",
          "icon": "images/icon.svg"
        }
      ]
    },
    "views": {
      "dataLineageViz": [
        {
          "id": "dataLineageViz.quickActions",
          "name": "Quick Actions",
          "icon": "$(graph)",
          "contextualTitle": "Data Lineage"
        }
      ]
    },
    "commands": [
      {
        "command": "dataLineageViz.open",
        "title": "Data Lineage: Open Wizard",
        "icon": "$(graph)"
      },
      {
        "command": "dataLineageViz.openDemo",
        "title": "Data Lineage: Open Demo",
        "icon": "$(play)"
      },
      {
        "command": "dataLineageViz.openSettings",
        "title": "Data Lineage: Settings",
        "icon": "$(gear)"
      },
      {
        "command": "dataLineageViz.createParseRules",
        "title": "Data Lineage: Create Parse Rules",
        "icon": "$(file-code)"
      },
      {
        "command": "dataLineageViz.createDmvQueries",
        "title": "Data Lineage: Create DMV Queries",
        "icon": "$(database)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "dataLineageViz.open",
          "when": "view == dataLineageViz.quickActions",
          "group": "navigation@1"
        }
      ]
    },
    "configuration": [
      {
        "title": "General",
        "properties": {
          "dataLineageViz.maxNodes": {
            "type": "number",
            "default": 500,
            "minimum": 10,
            "maximum": 1000,
            "description": "Maximum objects to import (10â€“1000).",
            "scope": "resource"
          }
        }
      },
      {
        "title": "Import",
        "properties": {
          "dataLineageViz.parseRulesFile": {
            "type": "string",
            "default": "",
            "editPresentation": "multilineText",
            "markdownDescription": "Path to custom `parseRules.yaml`. Leave empty for built-in defaults.",
            "scope": "resource"
          },
          "dataLineageViz.excludePatterns": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "default": [],
            "markdownDescription": "Regex patterns to exclude objects (e.g. `_TEST$`, `^staging\\\\.`).",
            "scope": "resource"
          }
        }
      },
      {
        "title": "Database Connection",
        "properties": {
          "dataLineageViz.dmvQueriesFile": {
            "type": "string",
            "default": "",
            "editPresentation": "multilineText",
            "markdownDescription": "Path to custom DMV queries YAML file. Leave empty to use built-in queries. See [dmvQueries.yaml](assets/dmvQueries.yaml) for format.",
            "scope": "resource"
          }
        }
      },
      {
        "title": "Layout",
        "properties": {
          "dataLineageViz.layout.direction": {
            "type": "string",
            "enum": [
              "TB",
              "LR"
            ],
            "default": "LR",
            "enumDescriptions": [
              "Top to Bottom (hierarchical)",
              "Left to Right (horizontal flow, default)"
            ],
            "markdownDescription": "Graph direction.",
            "scope": "resource"
          },
          "dataLineageViz.layout.rankSeparation": {
            "type": "number",
            "default": 120,
            "minimum": 20,
            "maximum": 300,
            "markdownDescription": "Space between layers.",
            "scope": "resource"
          },
          "dataLineageViz.layout.nodeSeparation": {
            "type": "number",
            "default": 30,
            "minimum": 10,
            "maximum": 200,
            "markdownDescription": "Space between nodes in the same layer.",
            "scope": "resource"
          },
          "dataLineageViz.edgeStyle": {
            "type": "string",
            "enum": [
              "default",
              "smoothstep",
              "step",
              "straight"
            ],
            "default": "default",
            "enumDescriptions": [
              "Smooth bezier curves (default)",
              "Smooth step edges with rounded corners",
              "Step edges with sharp corners",
              "Straight lines"
            ],
            "markdownDescription": "Edge/connection style.",
            "scope": "resource"
          },
          "dataLineageViz.layout.edgeAnimation": {
            "type": "boolean",
            "default": true,
            "markdownDescription": "Animate edges during trace.",
            "scope": "resource"
          },
          "dataLineageViz.layout.highlightAnimation": {
            "type": "boolean",
            "default": false,
            "markdownDescription": "Animate edges on node click.",
            "scope": "resource"
          },
          "dataLineageViz.layout.minimapEnabled": {
            "type": "boolean",
            "default": true,
            "markdownDescription": "Show minimap.",
            "scope": "resource"
          }
        }
      },
      {
        "title": "Trace",
        "properties": {
          "dataLineageViz.trace.defaultUpstreamLevels": {
            "type": "number",
            "default": 3,
            "minimum": 0,
            "maximum": 99,
            "markdownDescription": "Default upstream trace depth.",
            "scope": "resource"
          },
          "dataLineageViz.trace.defaultDownstreamLevels": {
            "type": "number",
            "default": 3,
            "minimum": 0,
            "maximum": 99,
            "markdownDescription": "Default downstream trace depth.",
            "scope": "resource"
          },
          "dataLineageViz.trace.hideCoWriters": {
            "type": "boolean",
            "default": true,
            "markdownDescription": "Hide sibling writers in trace results. Siblings are procedures that write to the same output table but aren't part of the traced data flow.",
            "scope": "resource"
          }
        }
      },
      {
        "title": "Analysis",
        "properties": {
          "dataLineageViz.analysis.hubMinDegree": {
            "type": "number",
            "default": 8,
            "minimum": 1,
            "maximum": 50,
            "markdownDescription": "Min connections to qualify as a hub.",
            "scope": "resource"
          },
          "dataLineageViz.analysis.islandMaxSize": {
            "type": "number",
            "default": 2,
            "minimum": 2,
            "maximum": 500,
            "markdownDescription": "Max size of disconnected subgraphs to show.",
            "scope": "resource"
          },
          "dataLineageViz.analysis.longestPathMinNodes": {
            "type": "number",
            "default": 5,
            "minimum": 2,
            "maximum": 50,
            "markdownDescription": "Min chain length for Longest Path analysis.",
            "scope": "resource"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build:ext && npm run build:webview",
    "build": "npm run build:ext && npm run build:webview",
    "build:ext": "node esbuild.config.mjs",
    "build:webview": "vite build",
    "dev": "vite",
    "watch": "npm run watch:ext",
    "watch:ext": "node esbuild.config.mjs --watch",
    "test": "tsx test/engine.test.ts && tsx test/parser-edge-cases.test.ts && tsx test/graphAnalysis.test.ts && tsx test/dmvExtractor.test.ts",
    "test:integration": "npm run build && tsx --tsconfig test/tsconfig.json test/runTest.ts"
  },
  "dependencies": {
    "@dagrejs/dagre": "^1.1.4",
    "@xyflow/react": "^12.4.0",
    "fast-xml-parser": "^5.3.4",
    "graphology": "^0.25.4",
    "graphology-components": "^1.5.4",
    "graphology-shortest-path": "^2.1.0",
    "graphology-traversal": "^0.3.1",
    "graphology-types": "^0.24.8",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.11.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/vscode": "^1.85.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vscode/test-electron": "^2.5.2",
    "autoprefixer": "^10.4.20",
    "esbuild": "^0.25.0",
    "glob": "^13.0.0",
    "mocha": "^11.7.5",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0"
  }
}
